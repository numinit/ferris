goog.require('zx');

goog.require('zx.Context');

(function(top) {
    var zx = top['zx'];

    if (zx._started) {
        return;
    }

    console.log('client starting');
    zx._started = true;
    zx._context = new zx.Context(top, '<%= ApplicationHelper.base_uri %>', '<%= asset_path "zx-worker.js" %>');
    zx._context.run();
})(this);

